{
    "neural-network": "N2",
    "no-augmentation": true,
    "movements": [
        "FigureofEight",
        "HighKneeJog",
        "Jog",
        "JumpingJacks",
        "SpeedSkater",
        "Static",
        "Zigzag",
        "TUG",
        "Walk"
    ],
    "train-subjects": [
        "S02",
        "S04",
        "S05",
        "S06",
        "S01",
        "S08",
        "S09",
        "S10"
    ],
    "validation-subjects": [
        "S07"
    ],
    "test-subjects": [
        "S07"
    ],
    "callbacks": {
        "enabled": true,
        "list": [
            {
                "type": "earlyStop",
                "monitor": "val_loss",
                "patience": 7,
                "mode": "min"
            },
            {
                "type": "modelCheckPoint",
                "save_weights_only":true,
                "monitor": "val_accuracy",
                "mode":"max",
                "save_best_only":true
            }
        ]
    },
    "input-rows": 350,
    "input-columns": 84,
    "channels": 1,
    "batch-size": 70,
    "train-steps": 1,
    "validation-steps": 1,
    "test-steps": 1,
    "epochs": 1
}